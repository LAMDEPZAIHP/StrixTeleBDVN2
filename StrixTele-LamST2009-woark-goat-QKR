local function v33(v57)
local v58=Instance.new("ScrollingFrame");
v58.Size=UDim2.new(1,0,1,0);
v58.BackgroundTransparency=1;
v58.ScrollBarThickness=8;
v58.AutomaticCanvasSize=Enum.AutomaticSize.Y;
v58.CanvasSize=UDim2.new();
v58.Visible=false;
v58.Parent=v27;
local v67=Instance.new("UIListLayout",v58);
v67.Padding=UDim.new(0,5);
v67.SortOrder=Enum.SortOrder.LayoutOrder;
local v71=Instance.new("UIPadding",v58);
v71.PaddingTop=UDim.new(0,5);
v71.PaddingBottom=UDim.new(0,10);
v71.PaddingLeft=UDim.new(0,5);
v71.PaddingRight=UDim.new(0,5);
for v80,v81 in ipairs(v57) do
local v82=v6(v58,v81[1],UDim2.new(1,-10,0,40),UDim2.new(),Color3.fromRGB(0,100,200));
v82.MouseButton1Click:Connect(function()
local v85=v0.Character or v0.CharacterAdded:Wait();
v85:PivotTo(v81[2]);
end);
end
return v58;
end

