local function v38(v76)
for v83,v84 in ipairs(v27:GetChildren()) do
if v84:IsA("ScrollingFrame") then
v84.Visible=v84==v76;
end
end
end

v24.MouseButton1Click:Connect(function() v38(v36);end);
v25.MouseButton1Click:Connect(function() v38(v37);end);

local v39=false;
v26.MouseButton1Click:Connect(function()
v39=not v39;
v26.Text="Auto Coin: "..((v39 and "ON") or "OFF");
v26.BackgroundColor3=(v39 and Color3.fromRGB(0,150,0)) or Color3.fromRGB(100,100,0);
end);

task.spawn(function()
while true do
if v39 then
for v87,v88 in ipairs(workspace:GetDescendants()) do
if ((v88.Name=="Respawning Coin") and v88:IsA("BasePart")) then
local v89=v0.Character or v0.CharacterAdded:Wait();
v89:PivotTo(v88.CFrame+Vector3.new(0,3,0));
task.wait(1);
if not v39 then break;end
end
end
end
task.wait(0.5);
end
end)

